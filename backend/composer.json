{
    "require": {
        "php": "^5.6 || ^7.0",
        "psy/psysh": "^0.9.5",
        "robmorgan/phinx": "^0.9.2",
        "symfony/console": "^3.4",
        "webmozart/assert": "^1.3",
        "zendframework/zend-component-installer": "^1.1",
        "zendframework/zend-crypt": "^3.3",
        "zendframework/zend-db": "^2.9",
        "zendframework/zend-form": "^2.12",
        "zendframework/zend-i18n": "^2.9",
        "zendframework/zend-i18n-resources": "^2.6",
        "zendframework/zend-math": "^3.1",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-mvc-i18n": "^1.1",
        "zendframework/zend-permissions-acl": "^2.7",
        "zendframework/zend-session": "^2.8",
        "zfcampus/zf-development-mode": "^3.0",
        "doctrine/doctrine-orm-module": "^1.1",
        "mikemix/tactician-module": "^2.0",
        "league/csv": "^9.1",
        "zendframework/zend-json": "^3.1",
        "nesbot/carbon": "^1.31"
    },
    
    "require-dev": {
        "zendframework/zend-test": "^3.2",
        "mockery/mockery": "^1.1",
        "fzaninotto/faker": "^1.7"
    },
    
    "autoload": {
        "psr-4": {
            "Base\\": "module/Base/src/",
            "FormPack\\": "module/FormPack/src/",
            "AdminAuth2\\": "module/AdminAuth2/src/",
            "DDD\\": "module/DDD/src/",
            "RenderCsv\\": "module/RenderCsv/src/",
            
            "App\\": "module/App/src/"
        }
    },
    
    "autoload-dev": {
        "psr-4": {
            "BackendTemplate\\": "module/BackendTemplate/src/",
            "BaseTest\\": "module/BaseTest/src/",
            
            "AppTest\\": "module/App/test/"
        }
    },
    
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php"
    },
    
    "extra": []
}
